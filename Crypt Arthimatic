from itertools import permutations

def solve_cryptarithmetic(word1, word2, result):
    letters = set(word1 + word2 + result)
    letters = list(letters)

    if len(letters) > 10:
        print("Too many unique letters")
        return

    for perm in permutations(range(10), len(letters)):
        mapping = dict(zip(letters, perm))

        # First letter should not be zero
        if mapping[word1[0]] == 0 or mapping[word2[0]] == 0 or mapping[result[0]] == 0:
            continue

        # Convert words to numbers
        num1 = int("".join(str(mapping[c]) for c in word1))
        num2 = int("".join(str(mapping[c]) for c in word2))
        num3 = int("".join(str(mapping[c]) for c in result))

        if num1 + num2 == num3:
            print("Solution Found:")
            print(mapping)
            print(f"{num1} + {num2} = {num3}")
            return

    print("No solution found")


# Example
solve_cryptarithmetic("SEND", "MORE", "MONEY")
